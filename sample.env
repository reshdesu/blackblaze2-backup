# BlackBlaze B2 Backup Tool - Sample Environment Template
# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env files to version control!

# BackBlaze B2 S3 Credentials
# Get these from your BackBlaze B2 account settings
# Go to: https://secure.backblaze.com/user_app_keys.htm
B2_ENDPOINT=s3.us-west-001.backblazeb2.com
B2_ACCESS_KEY_ID=your_access_key_id_here
B2_SECRET_ACCESS_KEY=your_secret_access_key_here
B2_REGION=us-west-001

# Test Configuration (optional)
TEST_BUCKET_NAME=your-test-bucket-name
TEST_FOLDER_PATH=/path/to/test/folder

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
